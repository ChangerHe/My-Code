{
    "version": "0.2.0",
    "configurations": [{
            "name": "Launch Chrome and new instance of Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "${workspaceRoot}",
            "sourceMaps": true,
            "webRoot": "D:/wamp/bin/apache/apache2.4.23"
        },
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 8080,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        }
    ]
}